\n                        return h(\r\n                            \"span\",\r\n                            params.index +\r\n                            (this.searchForm.pageNumber - 1) * this.searchForm.pageSize +\r\n                            1\r\n                        );\r\n                    },\r\n                },\r\n                {\r\n                    title: \"新闻标题\",\r\n                    key: \"newTitle\",\r\n                    minWidth: 120,\r\n                    sortable: false,\r\n                    fixed: \"left\",\r\n                },\r\n                {\r\n                    title: \"新闻描述\",\r\n                    key: \"newDescribe\",\r\n                    minWidth: 160,\r\n                    sortable: false,\r\n                    tooltip: true,\r\n                    render: (h, params) => {\r\n                        return h('pre', {\r\n                            domProps: {\r\n                                innerHTML: params.row.newDescribe\r\n                            }\r\n                        })\r\n                    }\r\n                },\r\n                {\r\n                    title: \"图片\",\r\n                    key: \"photo\",\r\n                    minWidth: 100,\r\n                    sortable: false,\r\n                    render: (h, params) => {\r\n                        if (params.row.photo != \"\") {\r\n                            return h(\"viewer\", [\r\n                                h(\"img\", {\r\n                                    attrs: {\r\n                                        src: params.row.photo,\r\n                                        height: 100,\r\n                                        width: \"100%\",\r\n                                    },\r\n                                }),\r\n                            ]);\r\n                        } else {\r\n                            let re = \"暂无照片\",\r\n                                color = \"red\";\r\n                     