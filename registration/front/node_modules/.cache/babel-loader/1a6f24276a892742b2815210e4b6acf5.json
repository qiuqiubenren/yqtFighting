amicNewTop\")) {\r\n                            return h(\"i-switch\", {\r\n                                props: {\r\n                                    size: \"large\",\r\n                                    value: params.row.isTop === \"yes\",\r\n                                },\r\n                                scopedSlots: {\r\n                                    open: () => h(\"span\", \"开启\"),\r\n                                    close: () => h(\"span\", \"关闭\"),\r\n                                },\r\n                                on: {\r\n                                    \"on-change\": (value) => {\r\n                                        setTopById({\r\n                                            id: params.row.id\r\n                                        }).then((res) => {\r\n                                            if (res.success) {\r\n                                                this.$Message.success(\"操作成功\");\r\n                                                this.getDataList();\r\n                                            }\r\n                                        });\r\n                                    },\r\n                                },\r\n                            });\r\n                        } else {\r\n                            let re = \"已置顶\",\r\n                                color = \"\";\r\n                            if (params.row.isTop == \"yes\") color = \"blue\";\r\n                            else {\r\n                                color = \"red\";\r\n                                re = \"非置顶\";\r\n                            }\r\n                            return h(\"div\", [\r\n                                h(\r\n                                    \"Tag\", {\r\n                                        props: {\r\n                                            color: color,\r\n                                        },\r\n                                    },\r\n                          